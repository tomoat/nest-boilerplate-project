    "@types/morgan": "^1.9.3",
    "@types/redis-rate-limiter": "^1.2.2",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.24.0",
    "base-64": "^1.0.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "cache-manager-redis-store": "^2.0.0",
